// GREEN PHASE: Minimal Templ for Preview mode results
package templates

templ PreviewModeResult(results string) {
  <div class="preview-results p-4 bg-base-100 rounded shadow">
    <h2 class="text-lg font-bold mb-2">Preview Results</h2>
    <pre class="whitespace-pre-wrap">{results}</pre>
  </div>
}
